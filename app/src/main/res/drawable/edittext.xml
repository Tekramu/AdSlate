<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item
        android:left="-4dp"
        android:right="-4dp"
        android:top="-4dp">
        <shape>
            <solid android:color="@android:color/transparent" />

            <stroke
                android:width="@dimen/line_width"
                android:color="@color/divider" />
        </shape>
    </item>

</layer-list>





